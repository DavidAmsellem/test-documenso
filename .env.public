# Configuración para desarrollo público accesible desde internet
# Copia de .env.example con modificaciones para acceso público

# [[AUTH]]
NEXTAUTH_SECRET="secret"

# [[CRYPTO]]
NEXT_PRIVATE_ENCRYPTION_KEY="CAFEBABE"
NEXT_PRIVATE_ENCRYPTION_SECONDARY_KEY="DEADBEEF"

# [[URLS - CONFIGURACIÓN PÚBLICA]]
NEXT_PUBLIC_WEBAPP_URL="http://xubuntu-server.duckdns.org:3000"
NEXT_PRIVATE_INTERNAL_WEBAPP_URL="http://xubuntu-server.duckdns.org:3000"

# [[DATABASE - CONFIGURACIÓN PÚBLICA CON PUERTOS DIFERENTES]]
NEXT_PRIVATE_DATABASE_URL="postgres://documenso:password@127.0.0.1:54322/documenso"
NEXT_PRIVATE_DIRECT_DATABASE_URL="postgres://documenso:password@127.0.0.1:54322/documenso"

# [[SIGNING]]
NEXT_PRIVATE_SIGNING_TRANSPORT="local"

# [[STORAGE - CONFIGURACIÓN PÚBLICA CON PUERTOS DIFERENTES]]
NEXT_PUBLIC_UPLOAD_TRANSPORT="database"
# Para usar MinIO público con puertos diferentes
NEXT_PRIVATE_UPLOAD_ENDPOINT="http://127.0.0.1:9102"
NEXT_PRIVATE_UPLOAD_FORCE_PATH_STYLE="false"
NEXT_PRIVATE_UPLOAD_REGION="unknown"
NEXT_PRIVATE_UPLOAD_BUCKET="documenso"
NEXT_PRIVATE_UPLOAD_ACCESS_KEY_ID="documenso"
NEXT_PRIVATE_UPLOAD_SECRET_ACCESS_KEY="password"

# [[SMTP - CONFIGURACIÓN PÚBLICA CON PUERTOS DIFERENTES]]
NEXT_PRIVATE_SMTP_TRANSPORT="smtp-auth"
NEXT_PRIVATE_SMTP_HOST="127.0.0.1"
NEXT_PRIVATE_SMTP_PORT=2501
NEXT_PRIVATE_SMTP_USERNAME="documenso"
NEXT_PRIVATE_SMTP_PASSWORD="password"
NEXT_PRIVATE_SMTP_SECURE=
NEXT_PRIVATE_SMTP_UNSAFE_IGNORE_TLS=
NEXT_PRIVATE_SMTP_FROM_NAME="Documenso"
NEXT_PRIVATE_SMTP_FROM_ADDRESS="noreply@xubuntu-server.duckdns.org"
NEXT_PRIVATE_SMTP_SERVICE=
NEXT_PRIVATE_RESEND_API_KEY=
NEXT_PRIVATE_MAILCHANNELS_API_KEY=
NEXT_PRIVATE_MAILCHANNELS_ENDPOINT=
NEXT_PRIVATE_MAILCHANNELS_DKIM_DOMAIN=
NEXT_PRIVATE_MAILCHANNELS_DKIM_SELECTOR=
NEXT_PRIVATE_MAILCHANNELS_DKIM_PRIVATE_KEY=
NEXT_PUBLIC_DOCUMENT_SIZE_UPLOAD_LIMIT=5

# [[STRIPE]]
NEXT_PRIVATE_STRIPE_API_KEY=
NEXT_PRIVATE_STRIPE_WEBHOOK_SECRET=

# [[BACKGROUND JOBS]]
NEXT_PRIVATE_JOBS_PROVIDER="local"
NEXT_PRIVATE_INNGEST_EVENT_KEY=

# [[SMS]]
SMS_PROVIDER="console"
TWILIO_ACCOUNT_SID=""
TWILIO_AUTH_TOKEN=""
TWILIO_PHONE_NUMBER=""

# [[FEATURES]]
NEXT_PUBLIC_POSTHOG_KEY=""
NEXT_PUBLIC_FEATURE_BILLING_ENABLED=
NEXT_PUBLIC_DISABLE_SIGNUP=

# [[E2E Tests]]
E2E_TEST_AUTHENTICATE_USERNAME="Test User"
E2E_TEST_AUTHENTICATE_USER_EMAIL="testuser@mail.com"
E2E_TEST_AUTHENTICATE_USER_PASSWORD="test_Password123"

# [[LOGGER]]
NEXT_PRIVATE_LOGGER_HONEY_BADGER_API_KEY=
